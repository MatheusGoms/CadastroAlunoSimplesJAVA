    private void btnLimparActionPerformed(java.awt.event.ActionEvent evt) {                                          
        txtMatricula.setText("");
        txtMedia.setText("0.0");
        txtNome.setText("");
        txtNota01.setText("");
        txtNota02.setText("");
    }                                         

    private void txtNota02FocusLost(java.awt.event.FocusEvent evt) {                                    
        if(!txtNota01.getText().equals("") && !txtNota02.getText().equals("")) {
            double nota01 = new Double(txtNota01.getText());
            double nota02 =  new Double(txtNota02.getText());
            Double media = (nota01 + nota02) /2;
            txtMedia.setText(media.toString());
        }
    }                                   

    private void txtNota01FocusLost(java.awt.event.FocusEvent evt) {                                    
        if(!txtNota01.getText().equals("") && !txtNota02.getText().equals("")) {
            double nota01 = new Double(txtNota01.getText());
            double nota02 =  new Double(txtNota02.getText());
            Double media = (nota01 + nota02) /2;
            txtMedia.setText(media.toString());
        }
    }                                   

    private void txtNota01KeyTyped(java.awt.event.KeyEvent evt) {                                   
        String caracteres="0987654321.";
            if(!caracteres.contains(evt.getKeyChar()+"")){
            evt.consume();
        }
    }                                  

    private void txtNota02KeyTyped(java.awt.event.KeyEvent evt) {                                   
        String caracteres="0987654321.";
            if(!caracteres.contains(evt.getKeyChar()+"")){
            evt.consume();
        }
    }                                  

